{
    "version": 2,
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/service-worker.js",
        "headers": {
          "Cache-Control": "no-cache"
        }
      },
      {
        "src": "/manifest.json",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "Cache-Control": "no-cache"
        }
      }
    ],
    "trailingSlash": false,
    "cleanUrls": true
  }